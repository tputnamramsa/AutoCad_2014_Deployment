<AecRecipeSpec defaultUnit="in">

    <Recipe idStr="section">
		<Label>Section</Label>
		<Implementation>
			<ObjectARX appnameAlias="AECRCPBASE" funcRcp="Stamp"/>
		</Implementation>
		<Data>
			<Item idStr="block">
				<AcadBlockProps>
				    <BlockName dwgLib="dwgs/AecDtlLib_Flashings.dwg" remapLayers="true" ref="dbTable">R_BLOCK</BlockName>
				</AcadBlockProps>
				<AcadEntityProps>
					<Layer ref="layerKey">THIN</Layer>
				</AcadEntityProps>
			</Item>
		</Data>
	</Recipe>

    <Recipe idStr="elevation">
		<Label>Elevation</Label>
		<Implementation>
			<ObjectARX appnameAlias="AECDTL" funcRcp="AecDtlRcpPreformedFlashingsElevation"/>
		</Implementation>
		<Controls>
            <CheckBox idStr="drawStartProfile">
                <Label>Draw start profile</Label>
                <Description>Specifies whether to draw the profile at the start of the component</Description>
                <Default>true</Default>
            </CheckBox>
            <CheckBox idStr="drawEndProfile">
                <Label>Draw end profile</Label>
                <Description>Specifies whether to draw the profile at the end point of the component</Description>
                <Default>true</Default>
            </CheckBox>
		</Controls>
		<Data>
			<Item idStr="boundary">
				<AcadEntityProps>
					<Layer ref="layerKey">THIN</Layer>
				</AcadEntityProps>
			</Item>
			<Item idStr="block">
				<AcadBlockProps>
				    <BlockName dwgLib="dwgs/AecDtlLib_Flashings.dwg" remapLayers="true" ref="dbTable">R_BLOCK_ELEVATION</BlockName>
				</AcadBlockProps>
				<AcadEntityProps>
					<Layer ref="layerKey">THIN</Layer>
				</AcadEntityProps>
			</Item>
		</Data>
	</Recipe>

</AecRecipeSpec>
