<AecRecipeSpec defaultUnit="in">

	<Recipe idStr="section">
		<Label>Section</Label>
		<Implementation>
			<ObjectARX appnameAlias="AECDTL" funcRcp="AecDtlRcpCurbMountable"/>
		</Implementation>
		<Controls>
			<CheckBox idStr="doHatch">
				<Label>Hatch item</Label>
			    <Description>Specifies whether to hatch the curb</Description>
				<Default>1</Default>
			</CheckBox>
		</Controls>
		<Data>
			<Item idStr="block">
				<AcadEntityProps>
					<Layer ref="layerKey">WIDE</Layer>
				</AcadEntityProps>
			</Item>
			<Item idStr="boundary">
				<Params>
					<Fillet>1.0</Fillet>
				</Params>
			</Item>
			<Item idStr="infill">
				<AcadHatchProps>
					<HatchAlias>concrete curb</HatchAlias>
				</AcadHatchProps>
				<AcadEntityProps>
					<Layer ref="layerKey">HATCH</Layer>
				</AcadEntityProps>
			</Item>
		</Data>
	</Recipe>
	
</AecRecipeSpec>
