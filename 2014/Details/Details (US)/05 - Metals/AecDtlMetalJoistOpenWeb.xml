<AecRecipeSpec defaultUnit="in">

	<Recipe idStr="section">
		<Label>Section</Label>
		<Implementation>
			<ObjectARX appnameAlias="AECDTL" funcRcp="AecDtlMetalJoistOpenWebSection"/>
		</Implementation>
		<Data>
			<Item idStr="block">
				<AcadEntityProps>
					<Layer ref="layerKey">THIN</Layer>
				</AcadEntityProps>
			</Item>
			<Item idStr="boundary">
				<AcadEntityProps>
					<Layer ref="layerKey">THIN</Layer>
				</AcadEntityProps>
				<Params>
					<Chord unit="mm">30.0</Chord >
					<Web unit="mm">13.0</Web>
					<Chord unit="in">1.25</Chord>
					<Web unit="in">0.5</Web>
				</Params>
			</Item>
			<Item idStr="interior">
				<AcadEntityProps>
					<Layer ref="layerKey">MED</Layer>
				</AcadEntityProps>
			</Item>
		</Data>
	</Recipe>
	
	<Recipe idStr="plan">
		<Label>Plan</Label>
		<Implementation>
			<ObjectARX appnameAlias="AECDTL" funcRcp="AecDtlMetalJoistOpenWebPlan"/>
		</Implementation>
		<Data>
			<Item idStr="block">
				<AcadEntityProps>
					<Layer ref="layerKey">MED</Layer>
				</AcadEntityProps>
			</Item>
			<Item idStr="breakline">
				<AcadEntityProps>
					<Layer ref="layerKey">ANNOBJ</Layer>
				</AcadEntityProps>
			</Item>
			<Item idStr="boundary">
				<AcadEntityProps>
					<Layer ref="layerKey">MED</Layer>
				</AcadEntityProps>
				<Params>
					<Chord unit="mm">30.0</Chord >
					<Web unit="mm">13.0</Web>
					<BearingDefault unit="mm">60.0</BearingDefault>
					<BearingDepth unit="mm">60.0</BearingDepth>
					<Chord unit="in">1.25</Chord>
					<Web unit="in">0.5</Web>
					<BearingDefault unit="in">2.5</BearingDefault>
					<BearingDepth unit="in">2.5</BearingDepth>
				</Params>
			</Item>
			<Item idStr="hidden">
				<AcadEntityProps>
					<Layer ref="layerKey">THIN</Layer>
					<Linetype>HIDDEN</Linetype>
				</AcadEntityProps>
			</Item>
		</Data>
	</Recipe>

	<Recipe idStr="elevation">
		<Label>Elevation</Label>
		<Implementation>
			<ObjectARX appnameAlias="AECDTL" funcRcp="AecDtlMetalJoistOpenWebElev"/>
		</Implementation>
		<Data>
			<Item idStr="block">
				<AcadEntityProps>
					<Layer ref="layerKey">MED</Layer>
				</AcadEntityProps>
			</Item>
			<Item idStr="breakline">
				<AcadEntityProps>
					<Layer ref="layerKey">ANNOBJ</Layer>
				</AcadEntityProps>
			</Item>
			<Item idStr="boundary">
				<AcadEntityProps>
					<Layer ref="layerKey">MED</Layer>
				</AcadEntityProps>
				<Params>
					<Chord unit="mm">30.0</Chord >
					<Web unit="mm">13.0</Web>
					<BearingDefault unit="mm">60.0</BearingDefault>
					<BearingDepth unit="mm">60.0</BearingDepth>
					<Chord unit="in">1.25</Chord>
					<Web unit="in">0.5</Web>
					<BearingDefault unit="in">2.5</BearingDefault>
					<BearingDepth unit="in">2.5</BearingDepth>
				</Params>
			</Item>
			<Item idStr="interior">
				<AcadEntityProps>
					<Layer ref="layerKey">THIN</Layer>
				</AcadEntityProps>
			</Item>
		</Data>
	</Recipe>
			
</AecRecipeSpec>

