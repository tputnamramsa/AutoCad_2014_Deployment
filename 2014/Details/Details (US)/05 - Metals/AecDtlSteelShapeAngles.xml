<AecRecipeSpec defaultUnit="in">

	<ControlDefs>
		<CheckBox idStr="doHatch">
			<Label>Hatch item</Label>
		    <Description>Specifies whether to hatch the angle</Description>
			<Default>1</Default>
		</CheckBox>
		<ComboBox idStr="orientation">
			<Label>Orientation</Label>
		    <Description>Specifies whether orientation is vertical or horizontal</Description>
			<ComboBoxValue idStr="vertical">
				<Label>Long leg vertical</Label>
			</ComboBoxValue>
			<ComboBoxValue idStr="horizontal">
				<Label>Long leg horizontal</Label>
			</ComboBoxValue>
		</ComboBox>
	</ControlDefs>
	
	<Recipe idStr="section">
		<Label>Section</Label>
		<Implementation>
			<ObjectARX appnameAlias="AECDTL" funcRcp="AecDtlRcpSteelAngleSection"/>
		</Implementation>
		<Controls>
			<ControlRef idRef="doHatch"/>
			<ControlRef idRef="orientation"/>
		</Controls>
		<Data>
			<Item idStr="block">
				<AcadEntityProps>
					<Layer ref="layerKey">WIDE</Layer>
				</AcadEntityProps>
			</Item>
			<Item idStr="boundary">
			</Item>
			<Item idStr="infill">
				<AcadHatchProps>
					<HatchAlias>metal shapes</HatchAlias>
				</AcadHatchProps>
				<AcadEntityProps>
					<Layer ref="layerKey">HATCH</Layer>
				</AcadEntityProps>
			</Item>
		</Data>
	</Recipe>
	
	<Recipe idStr="sectionDouble">
		<Label>Section (Double)</Label>
		<Implementation>
			<ObjectARX appnameAlias="AECDTL" funcRcp="AecDtlRcpSteelAngleSectionDouble"/>
		</Implementation>
		<Controls>
			<ControlRef idRef="doHatch"/>
			<ControlRef idRef="orientation"/>
			<EditBoxDistance idStr="spacing" unitDep="true">
				<Label>Spacing</Label>
			    <Description>Specifies the spacing</Description>
				<Default>0.5</Default>
				<Validation>NoNeg</Validation>
			</EditBoxDistance>
		</Controls>
		<Data>
			<Item idStr="block">
				<AcadEntityProps>
					<Layer ref="layerKey">WIDE</Layer>
				</AcadEntityProps>
			</Item>
			<Item idStr="boundary">
			</Item>
			<Item idStr="infill">
				<AcadHatchProps>
					<HatchAlias>metal shapes</HatchAlias>
				</AcadHatchProps>
				<AcadEntityProps>
					<Layer ref="layerKey">HATCH</Layer>
				</AcadEntityProps>
			</Item>
		</Data>
	</Recipe>
	
	<Recipe idStr="plan">
		<Label>Plan</Label>
		<Implementation>
			<ObjectARX appnameAlias="AECDTL" funcRcp="AecDtlRcpSteelAnglePlan"/>
		</Implementation>
		<Controls>
			<CheckBox idStr="hideFlanges">
				<Label>Hide flanges</Label>
    		    <Description>Specifies whether the flanges show as hidden lines</Description>
				<Default>0</Default>
			</CheckBox>
			<ControlRef idRef="orientation"/>
		</Controls>
		<Data>
			<Item idStr="boundary">
				<AcadEntityProps>
					<Layer ref="layerKey">MED</Layer>
				</AcadEntityProps>
			</Item>
			<Item idStr="hidden">
				<AcadEntityProps>
					<Layer ref="layerKey">THIN</Layer>
					<Linetype>HIDDEN</Linetype>
				</AcadEntityProps>
			</Item>
		</Data>
	</Recipe>
	<Recipe idStr="planDouble">
		<Label>Plan (Double)</Label>
		<Implementation>
			<ObjectARX appnameAlias="AECDTL" funcRcp="AecDtlRcpSteelAnglePlanDouble"/>
		</Implementation>
		<Controls>
			<CheckBox idStr="hideFlanges">
				<Label>Hide flanges</Label>
    		    <Description>Specifies whether the flanges show hidden lines</Description>
				<Default>0</Default>
			</CheckBox>
			<EditBoxDistance idStr="spacing" unitDep="true">
				<Label>Spacing</Label>
    		    <Description>Specifies spacing</Description>
				<Default>0.5</Default>
				<Validation>NoNeg</Validation>
			</EditBoxDistance>
			<ControlRef idRef="orientation"/>
		</Controls>
		<Data>
			<Item idStr="boundary">
				<AcadEntityProps>
					<Layer ref="layerKey">MED</Layer>
				</AcadEntityProps>
			</Item>
			<Item idStr="hidden">
				<AcadEntityProps>
					<Layer ref="layerKey">THIN</Layer>
					<Linetype>HIDDEN</Linetype>
				</AcadEntityProps>
			</Item>
		</Data>
	</Recipe>
	
	<Recipe idStr="elevation">
		<Label>Elevation</Label>
		<Implementation>
			<ObjectARX appnameAlias="AECDTL" funcRcp="AecDtlRcpSteelAngleElevation"/>
		</Implementation>
		<Controls>
			<CheckBox idStr="hideFlanges">
				<Label>Hide flanges</Label>
    		    <Description>Specifies whether the flanges show hidden lines</Description>
				<Default>0</Default>
			</CheckBox>
			<ControlRef idRef="orientation"/>
		</Controls>
		<Data>
			<Item idStr="boundary">
				<AcadEntityProps>
					<Layer ref="layerKey">MED</Layer>
				</AcadEntityProps>
			</Item>
			<Item idStr="hidden">
				<AcadEntityProps>
					<Layer ref="layerKey">THIN</Layer>
					<Linetype>HIDDEN</Linetype>
				</AcadEntityProps>
			</Item>
		</Data>
	</Recipe>
	
	<SubRecipe idStr="subPlan">
		<Label>Chord Angle</Label>
		<Implementation>
			<ObjectARX appnameAlias="AECDTL" funcRcp="AecDtlRcpSubSteelAnglePlan"/>
		</Implementation>
		<Controls>
			<ComboBox idStr="orientation">
				<Label>In plane legs</Label>
    		    <Description>Specifies the leg orientation</Description>
				<ComboBoxValue idStr="Long">
					<Label>Long</Label>
				</ComboBoxValue>
				<ComboBoxValue idStr="Short">
					<Label>Short</Label>
				</ComboBoxValue>
			</ComboBox>
		</Controls>
		<Data>
			<Item idStr="block">
				<AcadEntityProps>
					<Layer ref="layerKey">MED</Layer>
				</AcadEntityProps>
			</Item>
		</Data>
	</SubRecipe>
	
	<SubRecipe idStr="subElev1">
		<Label>Chord Angle</Label>
		<Implementation>
			<ObjectARX appnameAlias="AECDTL" funcRcp="AecDtlRcpSubSteelAnglePlan"/>
		</Implementation>
		<Controls>
			<ComboBox idStr="orientation">
				<Label>Chord angle outstanding leg</Label>
    		    <Description>Specifies which leg is used for the chord angle</Description>
				<ComboBoxValue idStr="Long">
					<Label>Long</Label>
				</ComboBoxValue>
				<ComboBoxValue idStr="Short">
					<Label>Short</Label>
				</ComboBoxValue>
				<Default>Short</Default>
			</ComboBox>
		</Controls>
		<Data>
			<Item idStr="block">
				<AcadEntityProps>
					<Layer ref="layerKey">MED</Layer>
				</AcadEntityProps>
			</Item>
		</Data>
	</SubRecipe>
	
	<SubRecipe idStr="subElev2">
		<Label>Web Angle</Label>
		<Implementation>
			<ObjectARX appnameAlias="AECDTL" funcRcp="AecDtlRcpSubSteelAnglePlan"/>
		</Implementation>
		<Controls>
			<ComboBox idStr="orientation">
				<Label>Web angle outstanding leg</Label>
    		    <Description>Specifies which leg is used for the web angle</Description>
				<ComboBoxValue idStr="Long">
					<Label>Long</Label>
				</ComboBoxValue>
				<ComboBoxValue idStr="Short">
					<Label>Short</Label>
				</ComboBoxValue>
				<Default>Long</Default>
			</ComboBox>
		</Controls>
		<Data>
			<Item idStr="block">
				<AcadEntityProps>
					<Layer ref="layerKey">MED</Layer>
				</AcadEntityProps>
			</Item>
		</Data>
	</SubRecipe>
	
	<SubRecipe idStr="subChordAngleSection">
		<Label>Chord Angle</Label>
		<Implementation>
			<ObjectARX appnameAlias="AECDTL" funcRcp="AecDtlRcpSubJoistSectionChord"/>
		</Implementation>
		<Controls>
			<ComboBox idStr="orientation">
				<Label>Chord angle outstanding leg</Label>
    		    <Description>Specifies which leg is used for the chord angle</Description>
				<ComboBoxValue idStr="Long">
					<Label>Long</Label>
				</ComboBoxValue>
				<ComboBoxValue idStr="Short">
					<Label>Short</Label>
				</ComboBoxValue>
				<Default>Short</Default>
			</ComboBox>
			<CheckBox idStr="doHatch">
				<Label>Hatch item</Label>
    		    <Description>Specifies whether the angle is hatched</Description>
				<Default>0</Default>
			</CheckBox>
		</Controls>
		<Data>
			<Item idStr="boundary">
				<AcadEntityProps>
					<Layer ref="layerKey">MED</Layer>
				</AcadEntityProps>
			</Item>
			<Item idStr="block">
				<AcadEntityProps>
					<Layer ref="layerKey">MED</Layer>
				</AcadEntityProps>
			</Item>
			<Item idStr="infill">
				<AcadHatchProps>
					<HatchAlias>metal shapes</HatchAlias>
				</AcadHatchProps>
				<AcadEntityProps>
					<Layer ref="layerKey">HATCH</Layer>
				</AcadEntityProps>
			</Item>
		</Data>
	</SubRecipe>
	
	<SubRecipe idStr="subWebAngleSection">
		<Label>Web Angle</Label>
		<Implementation>
			<ObjectARX appnameAlias="AECDTL" funcRcp="AecDtlRcpSubJoistSectionWeb"/>
		</Implementation>
		<Controls>
			<ComboBox idStr="orientation">
				<Label>Web angle outstanding leg</Label>
    		    <Description>Specifies which leg is used for the web angle</Description>
				<ComboBoxValue idStr="Long">
					<Label>Long</Label>
				</ComboBoxValue>
				<ComboBoxValue idStr="Short">
					<Label>Short</Label>
				</ComboBoxValue>
				<Default>Short</Default>
			</ComboBox>
		</Controls>
		<Data>
			<Item idStr="boundary">
				<AcadEntityProps>
					<Layer ref="layerKey">MED</Layer>
				</AcadEntityProps>
			</Item>
			<Item idStr="hidden">
				<AcadEntityProps>
					<Layer ref="layerKey">THIN</Layer>
					<Linetype>HIDDEN</Linetype>
				</AcadEntityProps>
			</Item>
			<Item idStr="block">
				<AcadEntityProps>
					<Layer ref="layerKey">MED</Layer>
				</AcadEntityProps>
			</Item>
		</Data>
	</SubRecipe>
	
</AecRecipeSpec>
