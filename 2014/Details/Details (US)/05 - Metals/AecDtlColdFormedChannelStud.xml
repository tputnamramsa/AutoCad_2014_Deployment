<AecRecipeSpec defaultUnit="in">
  <Recipe idStr="section">
    <Label>Section</Label>
    <Implementation>
      <ObjectARX appnameAlias="AECDTL" funcRcp="AecDtlRcpColdFormedChannelStudSection" />
    </Implementation>
    <Data>
      <Item idStr="block">
        <AcadEntityProps>
          <Layer ref="layerKey">EXTRAWIDE</Layer>
        </AcadEntityProps>
      </Item>
      <Item idStr="boundary" />
    </Data>
  </Recipe>
  <Recipe idStr="plan">
    <Label>Plan</Label>
    <Implementation>
      <ObjectARX appnameAlias="AECDTL" funcRcp="AecDtlRcpColdFormedChannelStudPlan" />
    </Implementation>
    <Data>
      <Item idStr="boundary">
        <AcadEntityProps>
          <Layer ref="layerKey">THIN</Layer>
        </AcadEntityProps>
      </Item>
      <Item idStr="hidden">
        <AcadEntityProps>
          <Layer ref="layerKey">FINE</Layer>
          <Linetype>HIDDEN</Linetype>
        </AcadEntityProps>
      </Item>
    </Data>
  </Recipe>
  <Recipe idStr="elevation">
    <Label>Elevation</Label>
    <Implementation>
      <ObjectARX appnameAlias="AECDTL" funcRcp="AecDtlRcpColdFormedChannelStudElevation" />
    </Implementation>
    <Controls>
      <CheckBox idStr="hideGauges">
        <Label>Hide gauge lines</Label>
        <Description>Specifies whether hidden lines are drawn</Description>
        <Default>1</Default>
      </CheckBox>
    </Controls>
    <Data>
      <Item idStr="boundary">
        <AcadEntityProps>
          <Layer ref="layerKey">THIN</Layer>
        </AcadEntityProps>
      </Item>
      <Item idStr="hidden">
        <AcadEntityProps>
          <Layer ref="layerKey">HIDDEN</Layer>
          <Linetype>HIDDEN</Linetype>
        </AcadEntityProps>
      </Item>
    </Data>
  </Recipe>
</AecRecipeSpec>