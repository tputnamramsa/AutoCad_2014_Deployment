<AecRecipeSpec defaultUnit="in">
	<ControlDefs>
	
		<EditBoxDistance idStr="length" unitDep="true">
			<Label>Length</Label>
			<Description>Specifies length of plate in X direction</Description>
			<Default unit="in">16.0</Default>
			<Default unit="mm">400.0</Default>
			<Validation>NoNegNoZero</Validation>
		</EditBoxDistance>
		
		<EditBoxDistance idStr="width" unitDep="true">
			<Label>Width</Label>
			<Description>Specifies length of plate in Y direction</Description>
			<Default unit="in">12.0000</Default>
			<Default unit="mm">300.0</Default>
			<Validation>NoNegNoZero</Validation>
		</EditBoxDistance>
		
		<EditBoxDistance idStr="holeDiameter" unitDep="true">
			<Label>Hole diameter</Label>
			<Description>Specifies hole diameter</Description>
			<Default>1.0625</Default>
			<Validation>NoNegNoZero</Validation>
		</EditBoxDistance>
		
		<EditBoxDistance idStr="spacing" unitDep="true">
			<Label>Hole spacing</Label>
			<Description>Specifies spacing between holes</Description>
			<Default>2.0</Default>
			<Validation>NoNegNoZero</Validation>
		</EditBoxDistance>
				
		<EditBoxDistance idStr="verticalSpacing" unitDep="true">
			<Label>Vertical spacing</Label>
			<Description>Specifies vertical spacing between holes (Y direction)</Description>
			<Default>2.0</Default>
			<Validation>NoNegNoZero</Validation>
		</EditBoxDistance>
		
		<EditBoxDistance idStr="horizontalSpacing" unitDep="true">
			<Label>Horizontal spacing</Label>
			<Description>Specifies horizontal spacing between holes (X direction)</Description>
			<Default>2.0</Default>
			<Validation>NoNegNoZero</Validation>
		</EditBoxDistance>
		
		<EditBoxInteger idStr="numHoles">
			<Label>Number of holes</Label>
			<Description>Specifies number of holes</Description>
			<Default>2</Default>
			<Validation>NoNegNoZero</Validation>
		</EditBoxInteger>
		
		<EditBoxInteger idStr="numHolesWidth">
			<Label>Number of holes width</Label>
			<Description>Specifies number of holes along the width</Description>
			<Default>2</Default>
			<Validation>NoNegNoZero</Validation>
		</EditBoxInteger>
		
		<EditBoxInteger idStr="numHolesLength">
			<Label>Number of holes length</Label>
			<Description>Specifies number of holes along the length</Description>
			<Default>2</Default>
			<Validation>NoNegNoZero</Validation>
		</EditBoxInteger>
		
	</ControlDefs>
	
		
	<Recipe idStr="top">
		<Label>Top</Label>
		<Implementation>
			<ObjectARX appnameAlias="AECDTL" funcRcp="AecRcpSteelPlateBaseTop"/>
		</Implementation>
		<Controls>
			<ControlRef idRef="width"/>
			<ControlRef idRef="length"/>
			<ControlRef idRef="holeDiameter"/>
			<ControlRef idRef="numHolesWidth"/>
			<ControlRef idRef="numHolesLength"/>
			<ControlRef idRef="horizontalSpacing"/>
			<ControlRef idRef="verticalSpacing"/>
		</Controls>
		<Data>
			<Item idStr="block">
				<AcadEntityProps>
					<Layer ref="layerKey">MED</Layer>
				</AcadEntityProps>
			</Item>
			<Item idStr="boundary">
				<AcadEntityProps>
					<Layer ref="layerKey">MED</Layer>
				</AcadEntityProps>
			</Item>
			<Item idStr="holes">
				<AcadEntityProps>
					<Layer ref="layerKey">MED</Layer>
				</AcadEntityProps>
			</Item>
		</Data>
	</Recipe>

	<Recipe idStr="width">
		<Label>Width</Label>
		<Implementation>
			<ObjectARX appnameAlias="AECDTL" funcRcp="AecRcpSteelPlateBaseWidth"/>
		</Implementation>
		<Controls>
			<ControlRef idRef="width"/>
			<ControlRef idRef="holeDiameter"/>
			<ControlRef idRef="numHoles"/>
			<ControlRef idRef="spacing"/>
		</Controls>
		<Data>
			<Item idStr="block">
				<AcadEntityProps>
					<Layer ref="layerKey">WIDE</Layer>
				</AcadEntityProps>
			</Item>
			<Item idStr="boundary">
				<AcadLayerInfo key="WIDE"/>
			</Item>
			<Item idStr="hidden">
				<AcadEntityProps>
					<Layer ref="layerKey">THIN</Layer>
					<Linetype>HIDDEN</Linetype>
				</AcadEntityProps>
			</Item>
		</Data>
	</Recipe>

	<Recipe idStr="length">
		<Label>Length</Label>
		<Implementation>
			<ObjectARX appnameAlias="AECDTL" funcRcp="AecRcpSteelPlateBaseLength"/>
		</Implementation>
		<Controls>
			<ControlRef idRef="length"/>
			<ControlRef idRef="holeDiameter"/>
			<ControlRef idRef="numHoles"/>
			<ControlRef idRef="spacing"/>
		</Controls>
		<Data>
			<Item idStr="block">
				<AcadEntityProps>
					<Layer ref="layerKey">WIDE</Layer>
				</AcadEntityProps>
			</Item>
			<Item idStr="boundary">
				<AcadLayerInfo key="WIDE"/>
			</Item>
			<Item idStr="hidden">
				<AcadEntityProps>
					<Layer ref="layerKey">THIN</Layer>
					<Linetype>HIDDEN</Linetype>
				</AcadEntityProps>
			</Item>
		</Data>
	</Recipe>

</AecRecipeSpec>
