<AecRecipeSpec defaultUnit="in">

    <ControlDefs>
        <CheckBox idStr="hidden">
            <Label>Hidden</Label>
			<Description>Specifies whether to display hidden linework</Description>
            <Default>false</Default>
        </CheckBox>
    </ControlDefs>

	<Recipe idStr="plan">
		<Label>Plan</Label>
		<Implementation>
			<ObjectARX appnameAlias="AECDTL" funcRcp="AecDtlRcpMasonryAnchorPencilRodPlan"/>
		</Implementation>
		<Controls>
		    <ControlRef idRef="hidden"/>
		</Controls>
		<Data>
			<Item idStr="boundary">
				<AcadEntityProps>
					<Layer ref="layerKey">FINE</Layer>
				</AcadEntityProps>
			</Item>
			<Item idStr="hidden">
				<AcadEntityProps>
					<Layer ref="layerKey">FINE</Layer>
					<Linetype>HIDDEN2</Linetype>
					<LinetypeScale>0.0625</LinetypeScale>
				</AcadEntityProps>
			</Item>
		</Data>
	</Recipe>

	<Recipe idStr="section">
		<Label>Section</Label>
		<Implementation>
			<ObjectARX appnameAlias="AECDTL" funcRcp="AecDtlRcpMasonryAnchorPencilRodSection"/>
		</Implementation>
		<Data>
			<Item idStr="block">
				<AcadEntityProps>
					<Layer ref="layerKey">FINE</Layer>
				</AcadEntityProps>
			</Item>
		</Data>
	</Recipe>
	
</AecRecipeSpec>
