<AecRecipeSpec defaultUnit="in">
    <ControlDefs>
		<EditBoxDistance idStr="width" unitDep="true">
		    <Label>Width</Label>
		    <Description>Specifies the offset distance</Description>
		    <Default unit="in">0.875</Default>
		    <Default unit="mm">22</Default>
		    <Validation>NoNegNoZero</Validation>
		</EditBoxDistance>
		<EditBoxDistance idStr="length" unitDep="true">
		    <Label>Length</Label>
		    <Description>Specifies the length of the tie at masonry</Description>
		    <Default unit="in">7</Default>
		    <Default unit="mm">90.0</Default>
		    <Validation>NoNegNoZero</Validation>
		</EditBoxDistance>
		<EditBoxDistance idStr="length2" unitDep="true">
		    <Label>Length</Label>
		    <Description>Specifies the length of the tie</Description>
		    <Default unit="in">7</Default>
		    <Default unit="mm">90.0</Default>
		    <Validation>NoNegNoZero</Validation>
		</EditBoxDistance>

		<CheckBox idStr="doHidden">
		    <Label>Hidden</Label>
			<Description>Specifies whether to display hidden linework</Description>
		    <Default>0</Default>
		</CheckBox>

        <EditBoxInteger idStr="rows">
            <Label>X axis</Label>
            <Description>Specifies the number of ties in the X direction</Description>
            <Default>2</Default>
            <Validation>NoNegNoZero</Validation>
        </EditBoxInteger>
        <EditBoxDistance idStr="colSpacing" unitDep="true">
            <Label>X axis spacing</Label>
            <Description>Specifies the spacing between ties in the X direction</Description>
            <Default>32.0</Default>
            <Validation>NoZero</Validation>
        </EditBoxDistance>
        <EditBoxInteger idStr="cols">
            <Label>Y axis</Label>
            <Description>Specifies the number of ties in the Y direction</Description>
            <Default>2</Default>
            <Validation>NoNegNoZero</Validation>
        </EditBoxInteger>
        <EditBoxDistance idStr="rowSpacing" unitDep="true">
            <Label>Y axis spacing</Label>
            <Description>Specifies the spacing between ties in the Y direction</Description>
            <Default>16.0</Default>
            <Validation>NoZero</Validation>
        </EditBoxDistance>
        <CheckBox idStr="doStagger">
            <Label>Stagger pattern</Label>
            <Description>Specifies whether to offset consecutive rows</Description>
            <Default>true</Default>
        </CheckBox>
        <EditBoxDistance idStr="offset" unitDep="true">
            <Label>Offset</Label>
            <Description>Specifies the offset distance</Description>
            <Default>16.0</Default>
            <Validation>NoZero</Validation>
		    <ParentControl idRef="doStagger">
			    <ParentControlState state="true" visible="yes" enabled="yes"/>
			    <ParentControlState state="false" visible="no" enabled="no"/>
		    </ParentControl>
		</EditBoxDistance>
    </ControlDefs>

	<Recipe idStr="plan">
		<Label>Plan</Label>
		<Implementation>
			<ObjectARX appnameAlias="AECDTL" funcRcp="AecDtlRcpDovetailSeismicAnchorPlan"/>
		</Implementation>
		<Controls>
			<ControlRef idRef="width"/>
			<ControlRef idRef="length"/>
			<ControlRef idRef="doHidden"/>
		</Controls>
		<Data>
			<Item idStr="block">
				<AcadEntityProps>
					<Layer ref="layerKey">FINE</Layer>
				</AcadEntityProps>
			</Item>
			<Item idStr="boundary">
				<AcadEntityProps>
					<Layer ref="layerKey">FINE</Layer>
				</AcadEntityProps>
				<Params>
        		    <ThroatMinWidth>0.625</ThroatMinWidth>
        		    <ThroatMaxWidth>1.0</ThroatMaxWidth>
        		    <ThroatHeight>1.0</ThroatHeight>
        		    <ClipOffset>1.0</ClipOffset>
        		    <ClipGap>0.625</ClipGap>
        		    <ClipLength>0.25</ClipLength>
        		    <ClipWidth>0.25</ClipWidth>
				</Params>
			</Item>
			<Item idStr="hidden">
				<AcadEntityProps>
					<Layer ref="layerKey">FINE</Layer>
					<Linetype>HIDDEN2</Linetype>
					<LinetypeScale>0.0625</LinetypeScale>
				</AcadEntityProps>
			</Item>
		</Data>
	</Recipe>

	<Recipe idStr="planArray">
		<Label>Plan (Array)</Label>
		<Implementation>
			<ObjectARX appnameAlias="AECDTL" funcRcp="AecDtlRcpDovetailSeismicAnchorPlanArray"/>
		</Implementation>
		<Controls>
			<ControlRef idRef="width"/>
			<ControlRef idRef="length"/>
			<ControlRef idRef="doHidden"/>
		    <EditBoxDistance idStr="spacing" unitDep="true">
		        <Label>Spacing</Label>
		        <Description>Specifies the spacing between ties</Description>
		        <Default unit="in">32.0</Default>
		        <Default unit="mm">400.0</Default>
		        <Validation>NoNeg</Validation>
		    </EditBoxDistance>
		</Controls>
		<Data>
			<Item idStr="block">
				<AcadEntityProps>
					<Layer ref="layerKey">FINE</Layer>
				</AcadEntityProps>
			</Item>
			<Item idStr="boundary">
				<AcadEntityProps>
					<Layer ref="layerKey">FINE</Layer>
				</AcadEntityProps>
				<Params>
        		    <ThroatMinWidth>0.625</ThroatMinWidth>
        		    <ThroatMaxWidth>1.0</ThroatMaxWidth>
        		    <ThroatHeight>1.0</ThroatHeight>
        		    <ClipOffset>1.0</ClipOffset>
        		    <ClipGap>0.625</ClipGap>
        		    <ClipLength>0.25</ClipLength>
        		    <ClipWidth>0.25</ClipWidth>
				</Params>
			</Item>
			<Item idStr="hidden">
				<AcadEntityProps>
					<Layer ref="layerKey">FINE</Layer>
					<Linetype>HIDDEN2</Linetype>
					<LinetypeScale>0.0625</LinetypeScale>
				</AcadEntityProps>
			</Item>
		</Data>
	</Recipe>
	
	<Recipe idStr="section">
		<Label>Section</Label>
		<Implementation>
			<ObjectARX appnameAlias="AECDTL" funcRcp="AecDtlRcpDovetailSeismicAnchorSection"/>
		</Implementation>
		<Controls>
			<ControlRef idRef="length2"/>
			<ControlRef idRef="doHidden"/>
		</Controls>
		<Data>
			<Item idStr="block">
				<AcadEntityProps>
					<Layer ref="layerKey">FINE</Layer>
				</AcadEntityProps>
			</Item>
			<Item idStr="boundary">
				<AcadEntityProps>
					<Layer ref="layerKey">FINE</Layer>
				</AcadEntityProps>
				<Params>
				    <!-- <AnchorOffset>0.0771</AnchorOffset> -->
				    <AnchorOffset>0.0</AnchorOffset>
        		    <ThroatHeight>1.0</ThroatHeight>
        		    <ClipOffset>1.0</ClipOffset>
        		    <ClipGap>0.125</ClipGap>
        		    <ClipLength>0.25</ClipLength>
        		    <ClipAngle>45</ClipAngle>
				</Params>
			</Item>
			<Item idStr="hidden">
				<AcadEntityProps>
					<Layer ref="layerKey">FINE</Layer>
					<Linetype>HIDDEN2</Linetype>
					<LinetypeScale>0.0625</LinetypeScale>
				</AcadEntityProps>
			</Item>
		</Data>
	</Recipe>

	<Recipe idStr="sectionArray">
		<Label>Section (Array)</Label>
		<Implementation>
			<ObjectARX appnameAlias="AECDTL" funcRcp="AecDtlRcpDovetailSeismicAnchorSectionArray"/>
		</Implementation>
		<Controls>
			<ControlRef idRef="length2"/>
			<ControlRef idRef="doHidden"/>
		    <EditBoxDistance idStr="spacing" unitDep="true">
		        <Label>Spacing</Label>
		        <Description>Specifies the spacing between ties</Description>
		        <Default unit="in">16.0</Default>
		        <Default unit="mm">400.0</Default>
		        <Validation>NoNegNoZero</Validation>
		    </EditBoxDistance>
		</Controls>
		<Data>
			<Item idStr="block">
				<AcadEntityProps>
					<Layer ref="layerKey">FINE</Layer>
				</AcadEntityProps>
			</Item>
			<Item idStr="boundary">
				<AcadEntityProps>
					<Layer ref="layerKey">FINE</Layer>
				</AcadEntityProps>
				<Params>
				    <!-- <AnchorOffset>0.0771</AnchorOffset> -->
				    <AnchorOffset>0.0</AnchorOffset>
        		    <ThroatHeight>1.0</ThroatHeight>
        		    <ClipOffset>1.0</ClipOffset>
        		    <ClipGap>0.125</ClipGap>
        		    <ClipLength>0.25</ClipLength>
        		    <ClipAngle>45</ClipAngle>
				</Params>
			</Item>
			<Item idStr="hidden">
				<AcadEntityProps>
					<Layer ref="layerKey">FINE</Layer>
					<Linetype>HIDDEN</Linetype>
					<LinetypeScale>0.0625</LinetypeScale>
				</AcadEntityProps>
			</Item>
		</Data>
		<Options>
		    <Jig>
		        <IsOrientedX>false</IsOrientedX>
		    </Jig>
		    <JigEnt>
		        <IsOrientedX>false</IsOrientedX>
		    </JigEnt>
		</Options>
	</Recipe>

	<Recipe idStr="elevation">
		<Label>Elevation</Label>
		<Implementation>
			<ObjectARX appnameAlias="AECDTL" funcRcp="AecDtlRcpDovetailSeismicAnchorElevation"/>
		</Implementation>
		<Controls>
			<ControlRef idRef="width"/>
			<ControlRef idRef="doHidden"/>
		</Controls>
		<Data>
			<Item idStr="block">
				<AcadEntityProps>
					<Layer ref="layerKey">FINE</Layer>
				</AcadEntityProps>
			</Item>
			<Item idStr="boundary">
				<AcadEntityProps>
					<Layer ref="layerKey">FINE</Layer>
				</AcadEntityProps>
				<Params>
        		    <ClipWidth>0.25</ClipWidth>
        		    <ClipLength>0.25</ClipLength>
        		    <ClipAngle>45</ClipAngle>
				</Params>
			</Item>
			<Item idStr="hidden">
				<AcadEntityProps>
					<Layer ref="layerKey">FINE</Layer>
					<Linetype>HIDDEN2</Linetype>
					<LinetypeScale>0.0625</LinetypeScale>
				</AcadEntityProps>
			</Item>
		</Data>
	</Recipe>

	<Recipe idStr="elevationPattern">
		<Label>Elevation (Pattern)</Label>
		<Implementation>
			<ObjectARX appnameAlias="AECDTL" funcRcp="AecDtlRcpDovetailSeismicAnchorElevationPattern"/>
		</Implementation>
		<Controls>
			<ControlRef idRef="width"/>
			<ControlRef idRef="doHidden"/>
			<ControlRef idRef="rows"/>
			<ControlRef idRef="colSpacing"/>
			<ControlRef idRef="cols"/>
			<ControlRef idRef="rowSpacing"/>
			<ControlRef idRef="doStagger"/>
			<ControlRef idRef="offset"/>
		</Controls>
		<Data>
			<Item idStr="block">
				<AcadEntityProps>
					<Layer ref="layerKey">FINE</Layer>
				</AcadEntityProps>
			</Item>
			<Item idStr="boundary">
				<AcadEntityProps>
					<Layer ref="layerKey">FINE</Layer>
				</AcadEntityProps>
				<Params>
        		    <ClipWidth>0.25</ClipWidth>
        		    <ClipLength>0.25</ClipLength>
        		    <ClipAngle>45</ClipAngle>
				</Params>
			</Item>
			<Item idStr="hidden">
				<AcadEntityProps>
					<Layer ref="layerKey">FINE</Layer>
					<Linetype>HIDDEN2</Linetype>
					<LinetypeScale>0.0625</LinetypeScale>
				</AcadEntityProps>
			</Item>
		</Data>
	</Recipe>

</AecRecipeSpec>
