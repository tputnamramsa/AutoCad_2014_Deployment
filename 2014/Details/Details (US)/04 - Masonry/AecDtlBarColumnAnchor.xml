			</Item>
		</Data>
		<Options>
		    <Jig>
		        <IsOrientedX>false</IsOrientedX>
		    </Jig>
		    <JigEnt>
		        <IsOrientedX>false</IsOrientedX>
		    </JigEnt>
		</Options>
	</Recipe>
    
