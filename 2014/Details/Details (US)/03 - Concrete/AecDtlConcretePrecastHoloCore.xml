<AecRecipeSpec defaultUnit="in">

	<ControlDefs>
		<CheckBox idStr="doHatch">
			<Label>Hatch beam</Label>
			<Default>1</Default>
		</CheckBox>
		<CheckBox idStr="showHidden">
			<Label>Hidden lines</Label>
			<Default>1</Default>
		</CheckBox>
	</ControlDefs>

	<Recipe idStr="section">
		<Label>Section</Label>
		<Implementation>
			<ObjectARX appnameAlias="AECDTL" funcRcp="AecDtlConcretePrecastHoloCoreSection"/>
		</Implementation>
		<Controls>
			<CheckBox idStr="doHatch">
				<Label>Hatch slab</Label>
				<Default>1</Default>
			</CheckBox>
		</Controls>
		<Data>
			<Item idStr="block">
				<AcadEntityProps>
					<Layer ref="layerKey">WIDE</Layer>
				</AcadEntityProps>
			</Item>
			<Item idStr="boundary">
			</Item>
			<Item idStr="holes">
				<Params>
					<DiameterRatio>0.6666667</DiameterRatio>
				</Params>
			</Item>
			<Item idStr="infill">
				<AcadHatchProps>
					<HatchAlias>concrete</HatchAlias>
				</AcadHatchProps>
				<AcadEntityProps>
					<Layer ref="layerKey">HATCH</Layer>
				</AcadEntityProps>
			</Item>
		</Data>
	</Recipe>
	
	<Recipe idStr="plan">
		<Label>Plan</Label>
		<Implementation>
			<ObjectARX appnameAlias="AECDTL" funcRcp="AecDtlConcretePrecastHoloCorePlan"/>
		</Implementation>
		<Controls>
			<ControlRef idRef="showHidden"/>
		</Controls>
		<Data>
			<Item idStr="boundary">
				<AcadEntityProps>
					<Layer ref="layerKey">MED</Layer>
				</AcadEntityProps>
			</Item>
			<Item idStr="holes">
				<Params>
					<DiameterRatio>0.6666667</DiameterRatio>
				</Params>
			</Item>
			<Item idStr="hidden">
				<AcadEntityProps>
					<Layer ref="layerKey">THIN</Layer>
					<Linetype>HIDDEN</Linetype>
				</AcadEntityProps>
			</Item>
		</Data>
	</Recipe>
	
	<Recipe idStr="elev">
		<Label>Elevation</Label>
		<Implementation>
			<ObjectARX appnameAlias="AECDTL" funcRcp="AecDtlConcretePrecastHoloCoreElevation"/>
		</Implementation>
		<Controls>
			<ControlRef idRef="showHidden"/>
		</Controls>
		<Data>
			<Item idStr="boundary">
				<AcadEntityProps>
					<Layer ref="layerKey">MED</Layer>
				</AcadEntityProps>
			</Item>
			<Item idStr="holes">
				<Params>
					<DiameterRatio>0.6666667</DiameterRatio>
				</Params>
			</Item>
			<Item idStr="hidden">
				<AcadEntityProps>
					<Layer ref="layerKey">THIN</Layer>
					<Linetype>HIDDEN</Linetype>
				</AcadEntityProps>
			</Item>
		</Data>
	</Recipe>
	
</AecRecipeSpec>
