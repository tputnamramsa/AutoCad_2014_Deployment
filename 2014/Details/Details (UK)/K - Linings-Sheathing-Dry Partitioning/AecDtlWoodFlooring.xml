<AecRecipeSpec defaultUnit="in">
	<Recipe idStr="section">
		<Label>Section</Label>
		<Implementation>
			<ObjectARX appnameAlias="AECDTL" funcRcp="AecDtlRcpWoodFlooring"/>
		</Implementation>
		<Data>
			<Item idStr="block">
				<AcadEntityProps>
					<Layer ref="layerKey">THIN</Layer>
				</AcadEntityProps>
			</Item>
			<Item idStr="boundary">
				<Params>
					<Tongue>0.125</Tongue>
					<GrooveSpacing>0.03125</GrooveSpacing>	<!-- should usually be the same as GapSpacing between each piece (listed below) -->
				</Params>
			</Item>
			<Item idStr="infill">
				<AcadHatchProps>
					<HatchAlias>wood floor</HatchAlias>
				</AcadHatchProps>
				<AcadEntityProps>
					<Layer ref="layerKey">HATCH</Layer>
				</AcadEntityProps>
			</Item>
		</Data>
		<Options>
			<JigEnt>
				<GapSpacing>0.03125</GapSpacing>
				<Width ref="dbTable">WIDTH</Width>
			</JigEnt>
		</Options>
	</Recipe>
</AecRecipeSpec>
