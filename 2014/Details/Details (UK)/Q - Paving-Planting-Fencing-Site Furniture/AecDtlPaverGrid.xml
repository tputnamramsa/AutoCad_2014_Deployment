<AecRecipeSpec defaultUnit="in">

	<Recipe idStr="section">
		<Label>Section</Label>
		<Implementation>
			<ObjectARX appnameAlias="AECDTL" funcRcp="AecDtlRcpGridPaver"/>
		</Implementation>
		<Data>
			<Item idStr="block">
				<AcadEntityProps>
					<Layer ref="layerKey">MED</Layer>
				</AcadEntityProps>
			</Item>
			<Item idStr="boundary">
			</Item>
			<Item idStr="infill">
				<AcadHatchProps>
					<HatchAlias>concrete paver</HatchAlias>
				</AcadHatchProps>
				<AcadEntityProps>
					<Layer ref="layerKey">HATCH</Layer>
				</AcadEntityProps>
			</Item>
			<Item idStr="soil">
				<AcadHatchProps>
					<HatchAlias>top soil</HatchAlias>
				</AcadHatchProps>
				<AcadEntityProps>
					<Layer ref="layerKey">HATCH</Layer>
				</AcadEntityProps>
			</Item>
			<Item idStr="grass">
				<AcadEntityProps>
					<Layer ref="layerKey">THIN</Layer>
				</AcadEntityProps>
				<Params>
					<RandomLow>0.25</RandomLow>	<!-- Fudge factors to move blades of grass -->
					<RandomHigh>0.5</RandomHigh>
				</Params>
			</Item>
		</Data>
	</Recipe>
	
</AecRecipeSpec>
