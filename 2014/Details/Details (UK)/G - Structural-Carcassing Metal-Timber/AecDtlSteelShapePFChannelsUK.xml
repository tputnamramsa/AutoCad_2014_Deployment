<AecRecipeSpec>

	<Recipe idStr="section">
		<Label>Section</Label>
		<Implementation>
      <ManagedDotNet moduleName="AecDetailsUK.dll" dirKey=""  funcRcp="AecDtlRcpSteelPFChannelSection"/>
    </Implementation>
		<Controls>
			<CheckBox idStr="doHatch">
				<Label>Hatch item</Label>
				<Default>1</Default>
			</CheckBox>
			<ComboBox idStr="insertMode">
				<Label>Insertion mode</Label>
				<ComboBoxValue idStr="top">
					<Label>Top</Label>
				</ComboBoxValue>
				<ComboBoxValue idStr="centroid">
					<Label>Centroid</Label>
				</ComboBoxValue>
				<Default>top</Default>
			</ComboBox>
		</Controls>
		<Data>
			<Item idStr="block">
				<AcadEntityProps>
					<Layer ref="layerKey">WIDE</Layer>
				</AcadEntityProps>
			</Item>
			<Item idStr="boundary">
			</Item>
			<Item idStr="infill">
				<AcadHatchProps>
					<HatchAlias>metal shapes</HatchAlias>
				</AcadHatchProps>
				<AcadEntityProps>
					<Layer ref="layerKey">HATCH</Layer>
				</AcadEntityProps>
			</Item>
		</Data>
	</Recipe>

	<Recipe idStr="plan">
		<Label>Plan</Label>
		<Implementation>
      <ManagedDotNet moduleName="AecDetailsUK.dll" dirKey=""  funcRcp="AecDtlRcpSteelPFChannelPlan"/>
		</Implementation>
		<Controls>
			<CheckBox idStr="hideWeb">
				<Label>Hide web</Label>
				<Default>1</Default>
			</CheckBox>
		</Controls>
		<Data>
			<Item idStr="boundary">
				<AcadEntityProps>
					<Layer ref="layerKey">MED</Layer>
				</AcadEntityProps>
			</Item>
			<Item idStr="hidden">
				<AcadEntityProps>
					<Layer ref="layerKey">THIN</Layer>
					<Linetype>HIDDEN</Linetype>
				</AcadEntityProps>
			</Item>
		</Data>
	</Recipe>

	<Recipe idStr="elevation">
		<Label>Elevation</Label>
		<Implementation>
      <ManagedDotNet moduleName="AecDetailsUK.dll" dirKey=""  funcRcp="AecDtlRcpSteelPFChannelElev"/>
    </Implementation>
		<Controls>
			<CheckBox idStr="hideFlanges">
				<Label>Hide flanges</Label>
				<Default>0</Default>
			</CheckBox>
		</Controls>
		<Data>
			<Item idStr="boundary">
				<AcadEntityProps>
					<Layer ref="layerKey">MED</Layer>
				</AcadEntityProps>
			</Item>
			<Item idStr="hidden">
				<AcadEntityProps>
					<Layer ref="layerKey">THIN</Layer>
					<Linetype>HIDDEN</Linetype>
				</AcadEntityProps>
			</Item>
		</Data>
	</Recipe>

</AecRecipeSpec>
