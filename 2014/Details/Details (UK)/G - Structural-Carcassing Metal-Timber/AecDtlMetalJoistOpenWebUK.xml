        <ManagedDotNet moduleName="AecDetailsUK.dll" dirKey=""  funcRcp="AecDtlMetalJoistOpenWebSection"/>
		</Implementation>
		<Data>
			<Item idStr="block">
				<AcadEntityProps>
					<Layer ref="layerKey">THIN</Layer>
				</AcadEntityProps>
			</Item>
			<Item idStr="boundary">
				<AcadEntityProps>
					<Layer ref="layerKey">THIN</Layer>
				</AcadEntityProps>
			</Item>
			<Item idStr="interior">
				<AcadEntityProps>
					<Layer ref="layerKey">MED</Layer>
				</AcadEntityProps>
			</Item>
		</Data>
	</Recipe>

	<Recipe idStr="plan">
		<Label>Plan</Label>
		<Implementation>
        <ManagedDotNet moduleName="AecDetailsUK.dll" dirKey=""  funcRcp="AecDtlMetalJoistOpenWebPlan"/>
		</Implementation>
		<Data>
			<Item idStr="block">
				<AcadEntityProps>
					<Layer ref="layerKey">MED</Layer>
				</AcadEntityProps>
			</Item>
			<Item idStr="breakline">
				<AcadEntityProps>
					<Layer ref="layerKey">ANNOBJ</Layer>
				</AcadEntityProps>
			</Item>
			<Item idStr="boundary">
				<AcadEntityProps>
					<Layer ref="layerKey">MED</Layer>
				</AcadEntityProps>
			</Item>
			<Item idStr="hidden">
				<AcadEntityProps>
					<Layer ref="layerKey">THIN</Layer>
					<Linetype>HIDDEN</Linetype>
				</AcadEntityProps>
			</Item>
		</Data>
	</Recipe>

	<Recipe idStr="elevation">
		<Label>Elevation</Label>
		<Implementation>
			<ObjectARX appnameAlias="AECDTL" funcRcp="AecDtlMetalJoistOpenWebElev"/>
		</Implementation>
		<Data>
			<Item idStr="block">
				<AcadEntityProps>
					<Layer ref="layerKey">MED</Layer>
				</AcadEntityProps>
			</Item>
			<Item idStr="breakline">
				<AcadEntityProps>
					<Layer ref="layerKey">ANNOBJ</Layer>
				</AcadEntityProps>
			</Item>
			<Item idStr="boundary">
				<AcadEntityProps>
					<Layer ref="layerKey">MED</Layer>
				</AcadEntityProps>
				<Params>
					<Chord unit="mm" ref="dbTable">DIM_D</Chord >
					<Web unit="mm">13.0</Web>
					<BearingDefault unit="mm" ref="dbTable">DIM_D</BearingDefault>
					<BearingDepth unit="mm" ref="dbTable">DIM_D2</BearingDepth>
					<Chord unit="in">1.25</Chord>
					<Web unit="in">0.5</Web>
					<BearingDefault unit="in">2.5</BearingDefault>
					<BearingDepth unit="in">2.5</BearingDepth>
				</Params>
			</Item>
			<Item idStr="interior">
				<AcadEntityProps>
					<Layer ref="layerKey">THIN</Layer>
				</AcadEntityProps>
			</Item>
		</Data>
	</Recipe>

