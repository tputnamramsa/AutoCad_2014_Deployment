<AecRecipeSpec defaultUnit="in">

	<Recipe idStr="section">
		<Label>Section</Label>
		<Implementation>
			<ObjectARX appnameAlias="AECDTL" funcRcp="AecDtlRcpLumberSection"/>
		</Implementation>
		<Controls>
			<ComboBox idStr="type">
				<Label>Type</Label>
				<ComboBoxValue idStr="lumber">
					<Label>Lumber</Label>
				</ComboBoxValue>
				<ComboBoxValue idStr="plank">
					<Label>Plank</Label>
				</ComboBoxValue>
				<ComboBoxValue idStr="block">
					<Label>Block</Label>
				</ComboBoxValue>
			</ComboBox>
		</Controls>
		<Data>
			<Item idStr="block">
				<AcadEntityProps>
					<Layer ref="layerKey">WIDE</Layer>
				</AcadEntityProps>
			</Item>
			<Item idStr="boundary">
			</Item>
			<Item idStr="cutMarkLumber">
				<AcadEntityProps>
					<Layer ref="layerKey">THIN</Layer>
				</AcadEntityProps>
			</Item>
			<Item idStr="cutMarkBlock">
				<AcadEntityProps>
					<Layer ref="layerKey">THIN</Layer>
				</AcadEntityProps>
			</Item>
			<Item idStr="cutMarkPlank">
				<AcadHatchProps>
					<HatchAlias>plank</HatchAlias>
				</AcadHatchProps>
				<AcadEntityProps>
					<Layer ref="layerKey">HATCH</Layer>
				</AcadEntityProps>
			</Item>
		</Data>
	</Recipe>
	
	<Recipe idStr="plan">
		<Label>Plan</Label>
		<Implementation>
			<ObjectARX appnameAlias="AECDTL" funcRcp="AecDtlRcpLumberPlan"/>
		</Implementation>
		<Data>
			<Item idStr="boundary">
				<AcadEntityProps>
					<Layer ref="layerKey">MED</Layer>
				</AcadEntityProps>
			</Item>
		</Data>
	</Recipe>
	
	<Recipe idStr="elev">
		<Label>Elevation</Label>
		<Implementation>
			<ObjectARX appnameAlias="AECDTL" funcRcp="AecDtlRcpLumberElev"/>
		</Implementation>
		<Data>
			<Item idStr="boundary">
				<AcadEntityProps>
					<Layer ref="layerKey">MED</Layer>
				</AcadEntityProps>
			</Item>
		</Data>
	</Recipe>
	
</AecRecipeSpec>
