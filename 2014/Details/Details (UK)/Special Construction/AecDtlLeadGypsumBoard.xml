<AecRecipeSpec defaultUnit="in">

	<Recipe idStr="section">
		<Label>Side</Label>
		<Implementation>
			<ObjectARX appnameAlias="AECDTL" funcRcp="AecDtlRcpLeadLinedGypsumBoard"/>
		</Implementation>
		<Controls>
			<CheckBox idStr="doHatch">
				<Label>Hatch item</Label>
				<Default>1</Default>
			</CheckBox>
		</Controls>
		<Data>
			<Item idStr="boundary">
				<AcadEntityProps>
					<Layer ref="layerKey">MED</Layer>
				</AcadEntityProps>
				<Params>
					<LeadThickness>0.0625</LeadThickness>
				</Params>
			</Item>
			<Item idStr="infill">
				<AcadHatchProps>
					<HatchAlias>lead board</HatchAlias>
				</AcadHatchProps>
				<AcadEntityProps>
					<Layer ref="layerKey">HATCH</Layer>
				</AcadEntityProps>
			</Item>
			<Item idStr="hidden">
				<AcadEntityProps>
					<Layer ref="layerKey">WIDE</Layer>
					<Linetype>HIDDEN</Linetype>
				</AcadEntityProps>
			</Item>
		</Data>
	</Recipe>
	
</AecRecipeSpec>
